{
  "name": "statickit-zeit",
  "scope": "levelhq",
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@now/node" },
    { "src": "connect.js", "use": "@now/node" },
    { "src": "callback.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/connect", "dest": "/connect.js" },
    { "src": "/callback", "dest": "/callback.js" }
  ],
  "env": {
    "ROOT_URL": "https://statickit-zeit.levelhq.now.sh",
    "STATICKIT_URL": "https://app.statickit.com",
    "STATICKIT_API_URL": "https://api.statickit.com",
    "STATICKIT_CLIENT_ID": "00db634c69528db4065463a6f8265561e416c40f50c08cd3092b34495e61f9f3",
    "STATICKIT_CLIENT_SECRET": "@statickit-zeit-client-secret"
  }
}
